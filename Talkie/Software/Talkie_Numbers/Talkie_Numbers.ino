void loop()
	{
	get_mode();

// if phrase mode whait here to trigger the phrase (but read mode change)
	if(digitalRead(LOOP)==0)

		{


		if(speaking == true)
			{
			while(digitalRead(TRIGGER)==1);
			delay(10); // debounce
			speaking=false; // not sure why it work without this !
			}





		do
			{
			get_mode();

			}

		while(digitalRead(TRIGGER)==0);
		speaking = true;



		}





	switch(mode)
		{

		case 0:
			digits();
			break;

		case 1:
			digitsF();
			break;
		case 2:
			bigNumbers();
			break;


		case 3:
			voltmeter();
			break;
		case 4:
			frequencemeter();
			break;
		case 5:
			alphabet();
			break;
		case 6:
			nato();
			break;
		case 7:
			strange();
			break;




		}
	}



