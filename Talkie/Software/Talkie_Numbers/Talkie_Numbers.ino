void get_mode()
	{
	/*
	0:digits male voice
	1:digitsfemale voice
	2:big numbers (from 0 to 9999999)
	3:voltmeter (reading the CV voltage)
	4:frequencemeter (just saying "Hertz" instead of "Volts")
	5:Spell the alphabet
	6:nato alphabet
	7:strange sounds
	*/

	mode = map(analogRead(1),-100,1000,0,7);
	display(mode);
	}

void display(int n)
	{
	digitalWrite(LedD, ((n >> 3) & 1) ? HIGH : LOW);
	digitalWrite(LedC, ((n >> 2) & 1) ? HIGH : LOW);
	digitalWrite(LedB, ((n >> 1) & 1) ? HIGH : LOW);
	digitalWrite(LedA, (n & 1) ? HIGH : LOW);
	}

void setup()
	{

	Serial.begin(9600);


	pinMode(LedA, OUTPUT);
	pinMode(LedB, OUTPUT);
	pinMode(LedC, OUTPUT);
	pinMode(LedD, OUTPUT);


	pinMode(BEND, INPUT_PULLUP);
	pinMode(LOOP, INPUT_PULLUP);

	pinMode(TRIGGER, INPUT);

	}
void digits()
	{
	int n = map(analogRead(0),0,1000,0,9);
	sayNumber(n);
	}



void digitsF()
	{
	int n = map(analogRead(0),0,1000,0,9);
